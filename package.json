{
  "name": "platzi-conf-store-web",
  "version": "1.0.0",
  "description": "This is a platzi conf's web store 🛒 to buy products of the platzi conf event. This is a project for practice and understand the reactjs ⚛️ and the react hooks 🪝",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.dev.js",
    "build": "webpack",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix",
    "firebase:deploy": "firebase deploy",
    "prefirebase:deploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zearkiatos/platzi-conf-store-web.git"
  },
  "keywords": [],
  "author": "Pedro Capriles <caprilespe@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zearkiatos/platzi-conf-store-web/issues"
  },
  "homepage": "https://github.com/zearkiatos/platzi-conf-store-web#readme",
  "dependencies": {
    "@react-google-maps/api": "^2.8.1",
    "axios": "^0.26.1",
    "firebase": "^9.6.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-paypal-button": "^4.1.1",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint",
      "pre-push": "npm run format && npm run lint"
    }
  }
